{
  "name": "inject-loader",
  "version": "5.0.0",
  "description": "A Webpack loader for injecting code into modules via their dependencies",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "clean": "del-cli lib types tmp",
    "build": "webpack --config config/webpack.config.js",
    "build:test": "webpack --config config/webpack.test.config.js",
    "build:test:esm": "webpack --config config/webpack.test.esm.config.js",
    "build:release": "yarn clean && yarn build",
    "security": "yarn audit",
    "lint:prettier": "prettier --list-different .",
    "lint:eslint": "eslint --cache .",
    "lint": "yarn lint:prettier && yarn lint:eslint",
    "fix:prettier": "yarn lint:prettier -- --write",
    "fix:eslint": "yarn lint:eslint -- --fix",
    "fix": "yarn fix:prettier && yarn fix:eslint",
    "pretest:unit": "yarn clean && yarn build && yarn build:test && yarn build:test:esm",
    "test:unit": "mocha tmp/testBundle.js tmp/testBundleESM.js --require source-map-support/register",
    "test:integration": "./script/integration_test",
    "test": "yarn test:unit && yarn test:integration",
    "prepare": "husky install && yarn build:release"
  },
  "files": [
    "*.md",
    "lib",
    "types"
  ],
  "author": "Justin Morris <desk@pixelbloom.com> (http://pixelbloom.com)",
  "contributors": [
    "Yoshihiro Okumura"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:orrisroot/inject-loader.git"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.10",
    "@babel/types": "^7.22.10",
    "@types/babel__core": "^7.20.1",
    "@types/node": "^18",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "babel-loader": "^9.1.3",
    "del": "^7.0.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jasmine": "^4.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.0",
    "mocha": "^10.2.0",
    "prettier": "^3.0.2",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "peerDependencies": {
    "webpack": "^5"
  },
  "keywords": [
    "webpack",
    "testing",
    "loader",
    "webpack-loader",
    "inject",
    "mock",
    "mocking"
  ]
}
